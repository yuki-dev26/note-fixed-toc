{
    "manifest_version": 3,
    "name": "note Fixed ToC",
    "version": "1.1.0",
    "description": "note記事の目次を画面右下に固定表示します",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://note.com/*"
            ],
            "js": [
                "src/modules/constants.js",
                "src/modules/utils.js",
                "src/modules/toc.js",
                "src/modules/floater.js",
                "src/modules/navigation.js",
                "src/main.js"
            ],
            "css": [
                "src/style.css"
            ],
            "run_at": "document_end"
        }
    ],
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}